<% if @article.errors.any? %>
<% @article.errors.each do |error|%>
<p><%= error.full_message %></p>
<% end %>
<% end %>

<%= form_with model: @article do |form| %>
<%= form.label :title%>
<%= form.text_field :title%>
<%= form.label :content%>
<%= form.text_field :content%>
<%= form.submit%>


<% end %>